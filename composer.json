{
    "name": "exedra/exedra-website",
    "type": "project",
    "license": "Proprietary",
    "description": "A job-recruiting website application",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "ext-curl": "*",
        "ext-fileinfo": "*",
        "ext-simplexml": "*",
        "ext-zip": "*",
        "beberlei/doctrineextensions": "^1.3",
        "doctrine/dbal": "^4.2",
        "doctrine/doctrine-bundle": "^2.8",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^3.3",
        "gedmo/doctrine-extensions": "^3.13",
        "google/recaptcha": "*",
        "knplabs/knp-paginator-bundle": "^6.3",
        "league/commonmark": "^2.1",
        "league/csv": "^9.21",
        "liip/imagine-bundle": "^2.12",
        "presta/sitemap-bundle": "^4.1",
        "pyrrah/gravatar-bundle": "*",
        "ramsey/uuid": "^4.7",
        "symfony/amazon-mailer": "7.2.*",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "7.2.*",
        "symfony/console": "7.2.*",
        "symfony/dotenv": "7.2.*",
        "symfony/event-dispatcher": "7.2.*",
        "symfony/expression-language": "7.2.*",
        "symfony/flex": "^2.0",
        "symfony/form": "7.2.*",
        "symfony/framework-bundle": "7.2.*",
        "symfony/html-sanitizer": "7.2.*",
        "symfony/http-client": "7.2.*",
        "symfony/intl": "7.2.*",
        "symfony/lock": "7.2.*",
        "symfony/mailer": "7.2.*",
        "symfony/messenger": "7.2.*",
        "symfony/mime": "7.2.*",
        "symfony/monolog-bundle": "^3.7",
        "symfony/polyfill-intl-messageformatter": "^1.12",
        "symfony/rate-limiter": "7.2.*",
        "symfony/runtime": "7.2.*",
        "symfony/security-bundle": "7.2.*",
        "symfony/stimulus-bundle": "^2.13",
        "symfony/string": "7.2.*",
        "symfony/translation": "7.2.*",
        "symfony/twig-bundle": "7.2.*",
        "symfony/ux-live-component": "^2.6",
        "symfony/ux-turbo": "^2.13",
        "symfony/validator": "7.2.*",
        "symfony/webpack-encore-bundle": "^2.0",
        "symfony/yaml": "7.2.*",
        "twig/extra-bundle": "^3.3",
        "twig/intl-extra": "^3.3",
        "twig/markdown-extra": "^3.3",
        "twig/string-extra": "^3.21",
        "twig/twig": "^2.12|^3.0",
        "vich/uploader-bundle": "^2.5"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^4.0",
        "phpstan/extension-installer": "^1.2",
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "7.2.*",
        "symfony/css-selector": "7.2.*",
        "symfony/debug-bundle": "7.2.*",
        "symfony/maker-bundle": "^1.52",
        "symfony/phpunit-bridge": "^7.3",
        "symfony/stopwatch": "7.2.*",
        "symfony/var-dumper": "7.2.*",
        "symfony/web-profiler-bundle": "7.2.*"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true,
            "phpstan/extension-installer": true
        },
        "platform": {
            "php": "8.2.0"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:warmup": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    }
}
