{% extends 'public.html.twig' %}

{% block stylesheets %}
    {{ parent() }}
{% endblock %}

{% block content %}

    <!-- Start Main Team Area  -->
    <div class="main-content" style="padding-top: 120px;">

        <!-- Start Contact page  -->
        <div class="rainbow-blog-area rainbow-section-gap">
            <div class="container">

                <section class="http-error">
                    <div class="row justify-content-center py-3 py-md-4 py-lg-5">
                        <div class="text-center">
                            <div class="http-error-main">
                                <h1 style="font-size: 100px;">404!</h1>
                                <h1 class="font-weight-bold text-dark">Seite nicht gefunden!</h1>
                                <p>Es tut uns leid, aber die Seite, nach der Sie gesucht haben, existiert nicht.</p>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-12">
                        <div class="row justify-content-center mt--50 mb--20">
                            <a href="{{ path('app_homepage_index') }}" class="btn-default btn-border">
                                Zur Website <i class="feather-arrow-right ms-2"></i>
                            </a>
                        </div>
                    </div>
                </section>

            </div>
        </div>

    </div>
    <!-- End Solutions Detail Area  -->

{% endblock %}

{% block javascripts %}

    <!--homepage init -->
    <script src="{{ asset('assets/js/pages/homepage.init.js') }}"></script>

    {{ parent() }}

{% endblock %}
